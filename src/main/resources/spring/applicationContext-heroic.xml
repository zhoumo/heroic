<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-3.2.xsd"
	default-autowire="byName">
	<bean id="baseService" class="mine.heroic.common.BaseService">
		<property name="cacheList">
			<list>
				<bean class="mine.heroic.common.cache.MemcachedService" />
				<bean class="mine.heroic.common.cache.RedisService" />
			</list>
		</property>
	</bean>
	<import resource="classpath*:/cache/applicationContext-memcached.xml" />
	<import resource="classpath*:/cache/applicationContext-redis.xml" />
</beans>